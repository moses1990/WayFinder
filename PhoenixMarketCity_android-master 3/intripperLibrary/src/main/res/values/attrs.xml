<?xml version="1.0" encoding="utf-8"?>
<resources >
      <declare-styleable name = "il_ShimmerFrameLayout" >
            <attr name = "il_auto_start" format = "boolean" />
            <attr name = "il_base_alpha" format = "float" />
            <attr name = "il_duration" format = "integer" />
            <attr name = "il_repeat_count" format = "integer" />
            <attr name = "il_repeat_delay" format = "integer" />
            <attr name = "il_repeat_mode" format = "enum" >
                  <enum name = "restart" value = "1" />
                  <enum name = "reverse" value = "2" />
            </attr >
            <attr name = "il_angle" format = "enum" >
                  <enum name = "cw_0" value = "0" />
                  <enum name = "cw_90" value = "90" />
                  <enum name = "cw_180" value = "180" />
                  <enum name = "cw_270" value = "270" />
            </attr >
            <attr name = "il_dropoff" format = "float" />
            <attr name = "il_fixed_width" format = "dimension" />
            <attr name = "il_fixed_height" format = "dimension" />
            <attr name = "il_intensity" format = "float" />
            <attr name = "il_relative_width" format = "float" />
            <attr name = "il_relative_height" format = "float" />
            <attr name = "il_shape" format = "enum" >
                  <enum name = "linear" value = "0" />
                  <enum name = "radial" value = "1" />
            </attr >
            <attr name = "il_tilt" format = "float" />
      </declare-styleable >
      <declare-styleable name = "il_MaterialRippleLayout" >
            <attr name = "il_mrl_rippleColor" format = "color" localization = "suggested" />
            <attr name = "il_mrl_rippleDimension" format = "dimension" localization = "suggested" />
            <attr name = "il_mrl_rippleOverlay" format = "boolean" localization = "suggested" />
            <attr name = "il_mrl_rippleAlpha" format = "float" localization = "suggested" />
            <attr name = "il_mrl_rippleDuration" format = "integer" localization = "suggested" />
            <attr name = "il_mrl_rippleFadeDuration" format = "integer" localization = "suggested" />
            <attr name = "il_mrl_rippleHover" format = "boolean" localization = "suggested" />
            <attr name = "il_mrl_rippleBackground" format = "color" localization = "suggested" />
            <attr name = "il_mrl_rippleDelayClick" format = "boolean" localization = "suggested" />
            <attr name = "il_mrl_ripplePersistent" format = "boolean" localization = "suggested" />
            <attr name = "il_mrl_rippleInAdapter" format = "boolean" localization = "suggested" />
            <attr name = "il_mrl_rippleRoundedCorners" format = "dimension" localization = "suggested" />
      </declare-styleable >

</resources >